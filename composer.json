{
    "name": "micro-module/value-object",
    "license": "proprietary",
    "type": "library",
    "description": "A PHP library/collection of classes aimed immutable objects.",
    "require": {
        "php": "^8.0",
        "marc-mabe/php-enum": "^4.0",
        "micro-module/geotools": "^0.8",
        "micro-module/laminas-validator": "^3.1",
        "moneyphp/money": "^4.0",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-mockery": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^9.4",
        "vimeo/psalm": "^4.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MicroModule\\ValueObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroModule\\ValueObject\\Tests\\Unit\\": "tests/unit"
        }
    }
}
